<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Authors>Daniel Morgan</Authors>
    <PackageId>TurboLinksCore</PackageId>
    <PackageProjectUrl>https://github.com/dmorganb/TurboLinksCore</PackageProjectUrl>
    <RepositoryUrl>https://github.com/dmorganb/TurboLinksCore.git</RepositoryUrl>
    <Description>Server side cooperation middleware for TurboLinks</Description>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.ViewFeatures" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
